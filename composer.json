{
  "name": "twint-ag/twint-magento-hyva-integration",
  "version": "9.9.9-dev",
  "description": "This module enables the use of Twint with HyvÃ¤ Checkout",
  "homepage": "https://twint.ch",
  "type": "magento2-module",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "TWINT",
      "email": "plugin@twint.ch",
      "homepage": "https://twint.ch",
      "role": "Maintainer"
    }
  ],
  "keywords": [
    "twint",
    "twint.ch",
    "switzerland",
    "checkout",
    "express checkout",
    "payment",
    "magento",
    "magento2",
    "hyva",
    "hyva-checkout",
    "hyva-themes"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "hyva-themes/magento2-hyva-checkout": "^1.3",
    "hyva-themes/magento2-theme-module": "^1.3",
    "magento/framework": ">=103.0.4",
    "magento/module-sales": ">=103.0.4",
    "magento/module-csp": "*",
    "twint-ag/sdk": "^1.7.0",
    "twint-ag/twint-magento-extension": "^1.1.0"
  },
  "require-dev": {
    "symplify/easy-coding-standard": "^12.1",
    "slevomat/coding-standard": "^8.15",
    "rector/rector": "^1.2",
    "phpspec/prophecy-phpunit": "^2.2",
    "phpunit/phpunit": "^10.5",
    "mockery/mockery": "^1.6",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "repositories": {
    "hyva-themes/hyva-checkout": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-checkout/checkout.git"
    },
    "hyva-themes/magento2-theme-module": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-themes/magento2-theme-module.git"
    },
    "hyva-themes/magento2-reset-theme": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-themes/magento2-reset-theme.git"
    },
    "hyva-themes/magento2-email-module": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-themes/magento2-email-module.git"
    },
    "hyva-themes/magento2-default-theme-csp": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-themes/magento2-default-theme-csp.git"
    },
    "hyva-themes/magento2-default-theme": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-themes/magento2-default-theme.git"
    },
    "hyva-themes/magento2-order-cancellation-webapi": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-themes/magento2-order-cancellation-webapi.git"
    },
    "hyva-themes/magento2-compat-module-fallback": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-themes/magento2-compat-module-fallback.git"
    },
    "hyva-themes/magento2-mollie-theme-bundle": {
      "type": "vcs",
      "url": "https://gitlab.hyva.io/hyva-themes/hyva-compat/magento2-mollie-theme-bundle.git"
    },
    "0": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Twint\\MagentoHyva\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "magento/composer-dependency-version-audit-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "magento/magento-composer-installer": true
    }
  }
}
